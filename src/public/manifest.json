{
    "manifest_version": 3,
    "name": "Seabass",
    "version": "1",
    "icons": {
        "256": "img/logo.png"
    },
    "permissions": ["storage", {
        "fileSystem": ["write", "retainEntries", "directory"]
    }],
    "app": {
        "background": {
            "scripts": ["chrome/background.js"]
        }
    },
    "minimum_chrome_version": "28"
}