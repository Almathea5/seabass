{
    "manifest_version": 2,
    "name": "Seabass code editor",
    "version": "0.5.0",
    "icons": {
        "128": "img/logo-chrome.png"
    },
    "permissions": ["storage", {
        "fileSystem": ["write", "retainEntries", "directory"]
    }],
    "app": {
        "background": {
            "scripts": ["chrome/background.js"]
        }
    },
    "short_name": "Seabass",
    "minimum_chrome_version": "56",
    "offline_enabled": true
}